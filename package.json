{
  "name": "severomorets",
  "version": "1.0.0",
  "description": "Severomorets - система голосовых звонков с мастер и slave приложениями",
  "type": "module",
  "main": "server/index.mjs",
  "scripts": {
    "init": "npx cordova platform add android",
    "build": "node build-script.js",
    "build:release": "node build-script.js --release",
    "build:master": "node build-script.js --master",
    "build:slave": "node build-script.js --slave",
    "server": "node server/index.mjs",
    "dev": "node server/index.mjs",
    "start": "node server/index.mjs",
    "test": "mocha test/**/*.test.mjs --timeout 10000",
    "test:watch": "mocha test/**/*.test.mjs --watch --timeout 10000",
    "test:all": "npm run test"
  },
  "keywords": [
    "android",
    "cordova",
    "webrtc",
    "socket.io",
    "voice-call"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.19.2",
    "socket.io": "^4.7.5"
  },
  "devDependencies": {
    "cordova": "^12.0.0",
    "cordova-android": "^14.0.1",
    "cordova-plugin-android-permissions": "^1.1.5",
    "mocha": "^10.2.0",
    "chai": "^4.3.10",
    "socket.io-client": "^4.7.5"
  },
  "cordova": {
    "platforms": [
      "android"
    ],
    "plugins": {
      "cordova-plugin-android-permissions": {}
    }
  }
}
